## Docker ##
PROJECT_NAME=express-authen-knex-s3-db-ts
DB_TOOL_PORT=180

## Environment ##
NODE_ENV=development

## Server ##
PORT=4000

## Setup jet-logger ##
JET_LOGGER_MODE=CONSOLE
JET_LOGGER_FILEPATH=jet-logger.log
JET_LOGGER_TIMESTAMP=TRUE
JET_LOGGER_FORMAT=LINE

## Authentication ##
COOKIE_DOMAIN=localhost
COOKIE_PATH=/
SECURE_COOKIE=false
JWT_SECRET=xxxxxxxxxxxxxx
COOKIE_SECRET=xxxxxxxxxxxxxx
# expires in 3 days
COOKIE_EXP=259200000

## S3 ##
S3_BUCKET=thuantq1-test
S3_PATH=image
S3_COPY_PATH=copy
S3_REGION=ap-southeast-1
SIGNED_URL_EXPIRE_SECONDS=300

## DB ##
DB_MASTER_HOST=localhost
DB_SLAVE_HOST=localhost
DB_USER=admin
DB_PASSWORD=admin
DB_NAME=admin
DB_PORT=3306

## CORS ##
CORS_ORIGIN_PORT=3000,13000

## Sentry logging ##
SENTRY_DNS=
